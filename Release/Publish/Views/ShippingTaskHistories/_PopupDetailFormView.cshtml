        </div>
      </fieldset>
    </form>
    <!--end shippingtaskhistory_form-->
    <div id="map" style="width: 100%; height: 100%;"></div>
    <div id="panel"></div>
    <!--========================================================================================-->
  </div>
  <!--end window body -->
</div>
<div id="windowfooter" style="padding:5px;">&nbsp;</div>
<!-- 弹出窗体form表单 -->
<!-- 弹出编辑对话框 -->
<script type="text/javascript">
  function loadmap() {
    console.log('loadmap');
    var start = $('#ZQSDMS').textbox('getValue');
    var end = $('#ZMDDMS').textbox('getValue');
    var router = $('#Location').textbox('getValue');
    var map = new AMap.Map('map', {
      resizeEnable: true, //是否监控地图容器尺寸变化
    });
    var truckOptions = {
      map: map,
      policy: 0,
      size: 1,
      city: start,
      panel: 'panel'
    };
    var driving = new AMap.TruckDriving(truckOptions);
    var path = [
      { keyword: start },//起点
      { keyword: end  }//终点
    ];
    if (router) {
      path.splice(1, 0, { keyword: router });
    }
    console.log(path);
    driving.search(path, function (status, result) {
      // result即是对应的货车导航信息，相关数据结构文档请参考 https://lbs.amap.com/api/javascript-api/reference/route-search#m_DrivingResult
      if (status === 'complete') {
        console.log('绘制货车路线完成')
      } else {
        console.error('获取货车规划数据失败：' + result)
      }
    });
  }

</script>
<style>
  #panel {
    position: fixed;
    background-color: white;
    max-height: 90%;
    overflow-y: auto;
    top: 250px;
    right: 25px;
    width: 280px;
  }

    #panel .amap-lib-driving {
      border-radius: 4px;
      overflow: hidden;
    }

</style>