                     style="width:68%"
                     type="text"
                     data-options="prompt:'@Html.DisplayNameFor(model => model.ZCPH)',
								                 required:true,validType: 'carNo',
                                 delay: 500,
                panelWidth: 500,
                mode: 'remote',
                method: 'get',
                url: '',
                idField: 'PlateNumber',
                textField: 'PlateNumber',
                pagination: true,
                clientPaging: false,
                columns: [[
                    { field: 'PlateNumber', title: '车牌号', width: 110, sortable: true },
