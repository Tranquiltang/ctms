			    </label>
            </section>
				 
	   </div>
	    
     <div class="row">
			<section class="col col-4">
            <label class="label"><span class="required">@Html.DisplayNameFor(model => model.Compensation)</span> </label>
            <label class="input">
            <input id="@Html.IdFor(model => model.Compensation)"
				   name="@Html.NameFor(model => model.Compensation)" 
                   placeholder="@Html.DisplayNameFor(model => model.Compensation)" 
				   value="@Html.ValueFor(model => model.Compensation)"
				   tabindex="6" required
				   type="text" 
				   />
                 <b class="tooltip tooltip-bottom-right">@Html.DisplayNameFor(model => model.Compensation)</b>
			    </label>
            </section>
				 
	    
            <section class="col col-4">
            <label class="label">@Html.DisplayNameFor(model => model.Remark) </label>
            <label class="input">
			<input id="@Html.IdFor(model => model.Remark)" 
				   name="@Html.NameFor(model => model.Remark)" 
                   placeholder="@Html.DisplayNameFor(model => model.Remark)" 
				   value="@Html.ValueFor(model => model.Remark)"
				   tabindex="7" 
				   type="text" 
				   />
             <b class="tooltip tooltip-bottom-right">@Html.DisplayNameFor(model => model.Remark)</b>
			    </label>
            </section>
				 
	    
   </div>
</fieldset>
</form>
