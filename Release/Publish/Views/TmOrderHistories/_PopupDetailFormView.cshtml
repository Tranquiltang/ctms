				   value="@Html.ValueFor(model => model.TOR_ID)"
				   tabindex="2" 
				   class="easyui-textbox" 
                   style="width:68%"
				   type="text" 
				   data-options="prompt:'@Html.DisplayNameFor(model => model.TOR_ID)',
								 required:false, 
                                 validType: 'length[8,20]'
                                 " />
  </div>
				      <!--FO状态-->
