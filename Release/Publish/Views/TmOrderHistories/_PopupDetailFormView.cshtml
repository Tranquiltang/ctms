				   name="@Html.NameFor(model => model.TRQ_ID)" 
				   value="@Html.ValueFor(model => model.TRQ_ID)"
				   tabindex="0" required
				   class="easyui-textbox" 
                   style="width:68%"
				   type="text" 
				   data-options="prompt:'@Html.DisplayNameFor(model => model.TRQ_ID)',
								 required:true, 
                                 validType: 'length[8,20]'
                                 " />
  </div>
				      <!--业务类型-->
